# vim:ft=swayconfig
font pango:monospace 15

input type:touchpad {
    natural_scroll true
    tap enabled
}

input type:keyboard {
    xkb_options "altwin:swap_alt_win,caps:escape"
}

output eDP-1 {
  resolution 2256x1504
  scale 1
}

bar main status_command i3status-rs ~/.config/i3status-rs/laptop.toml
